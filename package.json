{
  "name": "user-portal",
  "version": "2.1.0",
  "description": "tenxcloud dashbaord 2.0",
  "main": "app.js",
  "scripts": {
    "install": "npm install --registry=https://registry.npm.taobao.org",
    "start": "npm run dev",
    "staging": "cross-env NODE_ENV=staging RUNNING_MODE=standard node app.js",
    "build-dll": "webpack --config webpack.config.ddl",
    "dev": "npm run build-dll && cross-env NODE_ENV=development RUNNING_MODE=enterprise node app.js",
    "dev-std": "npm run build-dll && cross-env NODE_ENV=development RUNNING_MODE=standard node app.js",
    "build": "cross-env NODE_ENV=production RUNNING_MODE=enterprise sh build.sh",
    "build-std": "cross-env NODE_ENV=production RUNNING_MODE=standard sh build.sh",
    "pro": "cross-env NODE_ENV=production RUNNING_MODE=enterprise node app.js",
    "pro-std": "cross-env NODE_ENV=production RUNNING_MODE=standard node app.js",
    "docker-base-build": "docker build -t tenxcloud/user-portal:base-4.4.7 -f Dockerfile.base .",
    "build-backend": "cross-env NODE_ENV=production RUNNING_MODE=enterprise webpack --config webpack.config.backend.js --progress",
    "build-all": "cross-env NODE_ENV=production RUNNING_MODE=enterprise sh build.sh --build=all",
    "build-all-clean": "cross-env NODE_ENV=production RUNNING_MODE=enterprise sh build.sh --build=all --clean=all",
    "win": "npm run dev",
    "win-std": "npm run dev-std",
    "win-build": "npm run build",
    "win-build-std": "npm run build-std",
    "win-pro": "npm run pro",
    "win-pro-std": "npm run pro-std",
    "win-build-all": "npm run build-all",
    "win-build-all-clean": "npm run build-all-clean",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.tenxcloud.com:enterprise-2.0/user-portal.git"
  },
  "keywords": [
    "React",
    "Redux",
    "Less",
    "Fetch",
    "Node.js",
    "ES2016"
  ],
  "engines": {
    "node": ">= 4"
  },
